<h1><%= @user.username%>'s Tweets</h1>

<%@user.tweets.each do |tweet|%>
  <a href="/tweets/<%=tweet.id%>"><%=tweet.content%></a>
  <br><br>
<% end %>

<a href="/tweets/new"><button type="button" name="button">New Tweet</button></a>
<a href="/tweets"><button type="button" name="button">Home</button></a>
